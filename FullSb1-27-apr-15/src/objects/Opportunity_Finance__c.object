        <errorMessage>You must select a Scholarship Type in order to enter a value in Scholarship Amount.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Shanghai_Housing_Bursary_FULL</fullName>
        <active>false</active>
        <description>We have reached free housing capacity for Shanghai MBA. Please contact Sonja Hein</description>
        <errorConditionFormula>Program_Parsed__c = &apos;MBA&apos; &amp;&amp; ISPICKVAL(Housing_Accommodation__c, &apos;Shanghai - Single Room (1 Person) - 8 Months - Free&apos;) &amp;&amp; ( $User.Id &lt;&gt; &apos;005U0000000QKXk&apos;) &amp;&amp; ISCHANGED(Housing_Accommodation__c)</errorConditionFormula>
        <errorDisplayField>Housing_Accommodation__c</errorDisplayField>
