    <fields>
        <fullName>Hult_Fin_Aid_GoEd_Merit_of_Tuition__c</fullName>
        <formula>IF(Tuition_Fee__c&gt;0, 

if( Program_Parsed__c = &quot;EMBA&quot;, 

((If (text(GoEd_Financing_Status__c) &lt;&gt; &quot;Not Started&quot; &amp;&amp; not(isblank(text(GoEd_Financing_Status__c))), Go_Ed_Loan_Amount__c,0)) + Hult_Financial_Aid__c + Scholarship_Amount__c + Campus_Financial_Aid_I__c + Fin_Aid_Hotel_and_Flight__c +GoEd_Interest_Free_Fin_Aid__c)/(2*Tuition_Fee__c), 

((If (text(GoEd_Financing_Status__c) &lt;&gt; &quot;Not Started&quot; &amp;&amp; not(isblank(text(GoEd_Financing_Status__c))), Go_Ed_Loan_Amount__c,0)) + Hult_Financial_Aid__c + Scholarship_Amount__c + Campus_Financial_Aid_I__c + Fin_Aid_Hotel_and_Flight__c + GoEd_Interest_Free_Fin_Aid__c )/Tuition_Fee__c 
) 
,0)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Hult Fin Aid+GoEd+Merit % of Tuition</label>
        <precision>18</precision>
        <scale>2</scale>
        <type>Percent</type>
    </fields>
    <fields>
        <fullName>Hult_Fin_Aid_Merit_Rec_Average__c</fullName>
        <description>SFSUP-1020</description>
        <formula>IF(Tuition_Fee__c&gt;0, 

if( Program_Parsed__c = &quot;EMBA&quot;, 

(Hult_Financial_Aid__c + Fin_Aid_Hotel_and_Flight__c +Scholarship_Amount__c + GoEd_Interest_Free_Fin_Aid__c + Dean_Scholarship_Amount__c )/(2*Tuition_Fee__c), 
(Hult_Financial_Aid__c + Fin_Aid_Hotel_and_Flight__c + Scholarship_Amount__c + GoEd_Interest_Free_Fin_Aid__c + Dean_Scholarship_Amount__c )/Tuition_Fee__c),0)</formula>
        <label>Hult Fin Aid+Merit Rec Average</label>
        <precision>18</precision>
        <scale>2</scale>
        <type>Percent</type>
    </fields>
