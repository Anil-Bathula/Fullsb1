        <errorConditionFormula>Unlock_Student_Financial_Information__c  &amp;&amp; NOT(CONTAINS(stage__c , &apos;Confirmed&apos;)) &amp;&amp;  ISCHANGED(Unlock_Student_Financial_Information__c  )</errorConditionFormula>
