&amp;&amp;
OR(
Start_Term__c = &apos;September 2011&apos;,
Start_Term__c = &apos;September 2012&apos;,
Start_Term__c = &apos;September 2013&apos;,
AND(
   Start_Term__c = &apos;September 2014&apos;,
   Program_Parsed__c &lt;&gt; &apos;EMBA&apos;,
   $User.Id &lt;&gt; &apos;005U00000018c81&apos;,
   $User.Id &lt;&gt; &apos;005U0000002eOIm&apos;,
   $User.Id &lt;&gt; &apos;005U0000000S2Sn&apos;,
   $User.Id &lt;&gt; &apos;005U0000000QKXh&apos;,
   $User.Id &lt;&gt; &apos;005U0000003JrHs&apos;,
   $User.Id &lt;&gt; &apos;005U0000000QKGT&apos;
   ))</errorConditionFormula>
        <errorDisplayField>Go_Ed_Loan_Amount__c</errorDisplayField>
