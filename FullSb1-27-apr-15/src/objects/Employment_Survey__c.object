    <fields>
        <fullName>Company_Formula__c</fullName>
        <description>Merges each of the 4 company fields into one field for easy reporting on company name</description>
        <formula>IF( TS_Application__c &lt;&gt; &quot;&quot;, TS_Application__r.ts2__Account__r.Name ,
IF( Offer_Company_Name__c &lt;&gt; &quot;&quot; , Offer_Company_Name__c ,
IF( Offer_Company__c &lt;&gt; &quot;&quot; , Offer_Company__r.Name ,
IF( Name_of_Bus__c &lt;&gt; &quot;&quot; , Name_of_Bus__c ,
IF( Company_Bus__c &lt;&gt; &quot;&quot; , Company_Bus__r.Name , &quot;&quot; )))))</formula>
        <label>Company.</label>
        <type>Text</type>
    </fields>
    <fields>
        <fullName>Company_Name__c</fullName>
        <formula>IF( ISBLANK(TS_Application__c  ), 

IF (ISBLANK(Offer_Company__r.Name ), Offer_Company_Name__c , Offer_Company__r.Name),  TS_Application__r.ts2__Account__r.Name )</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Company Name</label>
        <type>Text</type>
