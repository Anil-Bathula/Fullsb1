    <fields>
        <fullName>Counter_Returning_to_Previous_Employer__c</fullName>
        <description>For reporting purposes only</description>
        <formula>IF(
  AND(
     ISPICKVAL( What_are_you_doing_now__c , &apos;Returning to my previous employer&apos; ) ,
     OR(
       NOT(ISBLANK( Previous_Job_Company_Name__c )) ,
       NOT(ISBLANK(  Previous_Job_Company__c  ))
       )
      ) ,
  TRUE , FALSE
  )</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Counter: Returning to Previous Employer</label>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>Counter_Still_Seeking_Employment__c</fullName>
        <description>For reporting purposes only</description>
        <formula>IF( 
   OR(
     ISPICKVAL( Are_you_currently_looking_for_a_job__c , &apos;Yes, but i havent received an offer&apos; ),
     ISPICKVAL( Are_you_currently_looking_for_a_job__c , &apos;Yes, have received an offer but will not accept it&apos;) ,
     ISPICKVAL( Are_you_currently_looking_for_a_job__c , &apos;Yes, I am seeking, my offer was withdrawn by company&apos;)
     ) ,
   TRUE , FALSE
  )</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Counter: Still Seeking Employment</label>
        <type>Checkbox</type>
