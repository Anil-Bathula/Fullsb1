    <startsWith>Vowel</startsWith>
    <validationRules>
        <fullName>Family_Business_or_Starting_a_Business</fullName>
        <active>true</active>
        <errorConditionFormula>(ISPICKVAL( What_are_you_doing_now__c ,&quot;Working for my family business&quot;) ||
ISPICKVAL( What_are_you_doing_now__c ,&quot;Starting a business&quot;)|| 
(ISPICKVAL( What_are_you_doing_now__c ,&quot;Working as an intern at a company&quot;)&amp;&amp; ISBLANK( TS_Internship_Application__c )) )
&amp;&amp;
(
ISBLANK( Start_Date__c )||
ISBLANK( Salary_if_any_in_USD__c )||
ISBLANK( Job_Title_Bus__c )||
(ISBLANK( Company_Bus__c )&amp;&amp;ISBLANK( Name_of_Bus__c ))
)
&amp;&amp;
ISPICKVAL(Status__c , &apos;Publish&apos;)</errorConditionFormula>
        <errorDisplayField>Name_of_Bus__c</errorDisplayField>
        <errorMessage>You are in &apos;publish&apos; mode. You have selected &apos;Family Business&apos;, &apos;Internship&apos; or &apos;Starting a Business&apos; in &apos;What are you doing now?&apos;. You must complete the fields in this section</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Family_Business_or_Starting_a_Business2</fullName>
        <active>true</active>
        <errorConditionFormula>AND (
	NOT(ISPICKVAL( What_are_you_doing_now__c ,&quot;Starting a business&quot;)),
	NOT(ISPICKVAL( What_are_you_doing_now__c ,&quot;Working for my family business&quot;)),
        NOT(ISPICKVAL( What_are_you_doing_now__c ,&quot;Working as an intern at a company&quot;)),
		OR(NOT(ISBLANK( Company_Bus__c )),
			NOT(ISBLANK( Name_of_Bus__c )) ,
			NOT(ISBLANK(TEXT( Industry_Bus__c ))),
			NOT(ISBLANK( City_Bus__c )),
			NOT(ISBLANK(TEXT( Type__c ))),
			NOT(ISBLANK( Start_Date__c )),
			NOT(ISBLANK( Salary_if_any_in_USD__c )),
			NOT(ISBLANK( Job_Title_Bus__c ))))</errorConditionFormula>
        <errorDisplayField>Company_Bus__c</errorDisplayField>
        <errorMessage>The student has not received a job offer based on your answer to &apos;What are you doing now?&apos; so the job detail fields should remain blank.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Family_or_Non_Family_Offer</fullName>
        <active>true</active>
        <description>Cannot enter job details into both the Family/Intern offer section as well as the standard offer section.</description>
        <errorConditionFormula>OR( NOT(ISBLANK( Company_Bus__c )) ,
    NOT(ISBLANK( Name_of_Bus__c )))
&amp;&amp;

OR( NOT(ISBLANK( Offer_Company_Name__c )) ,
    NOT(ISBLANK( Offer_Company__c )))</errorConditionFormula>
        <errorMessage>You cannot enter job offer details in both the standard job offer details section and the family/starting/internship offer section.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Name_of_business_lookup_OR_text</fullName>
        <active>true</active>
        <errorConditionFormula>NOT(ISBLANK(Name_of_Bus__c )) &amp;&amp;
NOT(ISBLANK( Company_Bus__c  ))</errorConditionFormula>
        <errorDisplayField>Company_Bus__c</errorDisplayField>
        <errorMessage>Please select the company name from this field or enter in the field below, but not both.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Non_TS_Company_Details_are_mandatory</fullName>
        <active>true</active>
        <description>Non TS</description>
        <errorConditionFormula>AND(
   OR (NOT(ISBLANK( Offer_Company_Name__c )) ,
       NOT(ISBLANK( Offer_Company__c ))) ,
   ISBLANK(TEXT( Industry__c )) ,
   ISPICKVAL(Status__c , &apos;Publish&apos;)
   )</errorConditionFormula>
        <errorDisplayField>Ownership_Type__c</errorDisplayField>
        <errorMessage>You are in &apos;publish&apos; mode. Please complete the Company Ownership Type and Industry fields</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Non_TS_Company_or_Name_Mandatory</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
	ISBLANK( TS_Application__c ) ,
	Can_t_find_on_TS__c =FALSE,
	NOT(ISBLANK( Job_Title__c )),
	ISBLANK( Offer_Company__c ),
	ISBLANK( Offer_Company_Name__c ))
&amp;&amp;
ISPICKVAL(Status__c , &apos;Publish&apos;)</errorConditionFormula>
        <errorDisplayField>Offer_Company__c</errorDisplayField>
        <errorMessage>You are in &apos;publish&apos; mode. You have entered a Job Title but no company name. Please enter the company name to proceed.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Non_TS_Job_Title_Mandatory</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
	ISBLANK( TS_Application__c ) ,
	Can_t_find_on_TS__c =FALSE,
		OR(
			NOT(ISBLANK( Offer_Company__c )),
			NOT(ISBLANK( Offer_Company_Name__c ))),
	ISBLANK( Job_Title__c ))
&amp;&amp;
ISPICKVAL(Status__c , &apos;Publish&apos;)</errorConditionFormula>
        <errorDisplayField>Job_Title__c</errorDisplayField>
        <errorMessage>You are in &apos;publish&apos; mode. You have entered a company name but no Job Title, please enter the job title to proceed.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Non_TS_Offer_Job_Details_are_mandatory</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
NOT(ISBLANK( Job_Title__c) ),
OR(
ISBLANK( Job_City__c  ),
ISBLANK(TEXT( Job_Function__c ) ),
ISBLANK( TEXT( Job_Industry__c  ))
))
&amp;&amp;
ISPICKVAL(Status__c , &apos;Publish&apos;)</errorConditionFormula>
        <errorDisplayField>Job_Function__c</errorDisplayField>
        <errorMessage>You are in &apos;publish&apos; mode. Please complete all Job details (City, Job Function, Job Industry)</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Offer_Company_lookup_field_or_text</fullName>
        <active>true</active>
        <errorConditionFormula>NOT(ISBLANK( Offer_Company__c )) &amp;&amp;
NOT(ISBLANK( Offer_Company_Name__c ))</errorConditionFormula>
        <errorDisplayField>Offer_Company__c</errorDisplayField>
        <errorMessage>Please select the company name from this field or enter in the field below, but not both.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Offer_Dates_Hierachy</fullName>
        <active>true</active>
        <description>Accepted date cannot be greater than Start Date and Offer Date cannot be greater than Accepted Date.</description>
        <errorConditionFormula>(Offer_Accepted_Date__c &gt;  Employment_Start_Date__c
||
Offer_Received_Date__c &gt;  Offer_Accepted_Date__c)</errorConditionFormula>
        <errorDisplayField>Offer_Received_Date__c</errorDisplayField>
        <errorMessage>Offer Accepted Date must be before Employment Start Date.
Offer Received Date must be before the Offer Accepted Date</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Offer_Undecided_Draft_Mode</fullName>
        <active>true</active>
        <description>Undecided job offers must be saved in &apos;draft&apos; mode.</description>
        <errorConditionFormula>ISPICKVAL( Status__c , &apos;publish&apos; ) 
&amp;&amp; 
ISPICKVAL( Are_you_currently_looking_for_a_job__c , &apos;have an offer but undecided&apos;)</errorConditionFormula>
        <errorDisplayField>Status__c</errorDisplayField>
        <errorMessage>You cannot publish a job offer whilst it is undecided. Please select status = draft. When the student has decided to accept or reject the offer, please return and edit the employment survey with updated info and change status to &apos;publish&apos;.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Offer_dates_mandatory</fullName>
        <active>true</active>
        <description>Offer receive and accept dates are mandatory if entering job offer</description>
        <errorConditionFormula>AND( 
OR( 
NOT(ISBLANK( TS_Application__c )) , 
NOT(ISBLANK( Offer_Company__c )) , 
NOT(ISBLANK( Offer_Company_Name__c )) 
) , 
OR( 
ISBLANK( Offer_Accepted_Date__c ) , 
ISBLANK( Offer_Received_Date__c )
) 
)
&amp;&amp;
ISPICKVAL(Status__c , &apos;Publish&apos;)
&amp;&amp;
ISPICKVAL(What_are_you_doing_now__c , &apos;I have already accepted an offer&apos;)</errorConditionFormula>
        <errorDisplayField>Offer_Received_Date__c</errorDisplayField>
        <errorMessage>You are in &apos;publish&apos; mode. If you are entering a job offer, you must complete the job offer date fields.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Previous_Job_Company_lookup_OR_text</fullName>
        <active>true</active>
        <errorConditionFormula>NOT(ISBLANK( Previous_Job_Company__c )) &amp;&amp;
NOT(ISBLANK ( Previous_Job_Company_Name__c ))</errorConditionFormula>
        <errorDisplayField>Previous_Job_Company__c</errorDisplayField>
        <errorMessage>Please select the company name from this field or enter in the field below, but not both.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>What_are_you_doing_now_is_mandatory</fullName>
        <active>true</active>
        <errorConditionFormula>ISBLANK(TEXT(What_are_you_doing_now__c ))
&amp;&amp;
ISPICKVAL(Status__c , &apos;Publish&apos;)</errorConditionFormula>
        <errorDisplayField>What_are_you_doing_now__c</errorDisplayField>
        <errorMessage>You are in &apos;publish&apos; mode. Please select an answer for this question.</errorMessage>
    </validationRules>
</CustomObject>
