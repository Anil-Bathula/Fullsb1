        <label>Conversion Rate to USD</label>
        <precision>18</precision>
        <required>true</required>
        <scale>10</scale>
        <type>Number</type>
    </fields>
    <fields>
        <fullName>HCP_Currency_Code__c</fullName>
        <label>HCP Currency Code</label>
        <length>3</length>
        <required>true</required>
        <type>Text</type>
        <unique>true</unique>
    </fields>
    <label>HCP Currencies</label>
