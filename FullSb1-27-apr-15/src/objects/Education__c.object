    <validationRules>
        <fullName>AAM_Validation_for_Bacherlors_Only</fullName>
        <active>true</active>
        <description>AAMs can only validate &apos;Bacherlor&apos;s Degree&apos; records. by Daniel Mora</description>
        <errorConditionFormula>AND(AAM_Validated__c = TRUE,  text(Highest_Degree_Attained__c) &lt;&gt; &quot;Bachelor&apos;s Degree&quot;)</errorConditionFormula>
        <errorMessage>Only Bachelor&apos;s Degrees can be &apos;AAM Validated&apos;</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Enter_GPA</fullName>
        <active>true</active>
        <description>This validation is to check the converted GPA field should have value.
 when highest degree is bachelors and validate__AAM=True.</description>
        <errorConditionFormula>AND(ISPICKVAL( Highest_Degree_Attained__c ,&quot;Bachelor&apos;s Degree&quot;),AAM_Validated__c=True ,ISNULL(Converted_GPA__c))</errorConditionFormula>
        <errorMessage>Please enter a Converted GPA value before you validate this record</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>GPA_Must_not_be_more_than_4</fullName>
        <active>true</active>
        <description>To restrict the user from entering the GPA value More than 4 and less than 1.
The GPA value Should be between 1-4</description>
        <errorConditionFormula>OR(Converted_GPA__c&lt;1,Converted_GPA__c&gt;4)</errorConditionFormula>
        <errorMessage>Please enter GPA between 1 - 4</errorMessage>
    </validationRules>
