    <script> 
        function showHide(str){
           // alert(str);
            if(str=='page:disp:handicap:0'){
                //var pickval=document.getElementById(str).value;
                //if(pickval=='true'){
                document.getElementById('hcap').style.display='table-row';
                document.getElementById('hcap1').style.display='table-row';
                
            }
            else if(str=='page:disp:handicap:1'){
                document.getElementById('hcap').style.display='none';
                document.getElementById('hcap1').style.display='none';
            }
            else if(str=='page:disp:dietary:0'){
                    document.getElementById('diet').style.display='table-row';
                    document.getElementById('diet1').style.display='table-row';
            }
            else if(str=='page:disp:dietary:1'){
                document.getElementById('diet').style.display='none';
                document.getElementById('diet1').style.display='none';
            }
            else if(str=='page:disp:allergies:0'){
                    document.getElementById('alerg').style.display='table-row';
                    document.getElementById('alerg1').style.display='table-row';
            }
            else if(str=='page:disp:allergies:1'){
                document.getElementById('alerg').style.display='none';
                document.getElementById('alerg1').style.display='none';
            }
            
        }
        
